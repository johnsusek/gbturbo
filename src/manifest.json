{
  "name": "gbturbo",
  "description": "Giant Bomb Turbo",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "gbturbo",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "inject/ui.js"
      ],
      "matches": [
        "https://www.giantbomb.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}